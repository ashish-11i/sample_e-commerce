spring.datasource.url = jdbc:mysql://localhost:3306/e_commerce_website
spring.datasource.username = mysql
spring.datasource.password = mysql

#Every DB has own driver
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver

#It is used to execute create, update, .. query in db (like for entity)
spring.jpa.hibernate.ddl-auto=update

#It is used ti show sql query on the console
spring.jpa.show-sql=true

#Every database has own dialect means evary DB has own sql query
spring.jpa.properties.hibernate.dialect = org.hibernate.dialect.MySQL8Dialect

#For formating sql
spring.jpa.properties.hibernate.format_sql=true

#For log and debug
#logging.level.org.springframework.security=debug

secret=somerandomsecret

